{
  "name": "garvinhicking/clinspector-gadget",
  "type": "typo3-cms-extension",
  "description": "Very, very simple TYPO3 console CLI command JSON export",
  "keywords": [
    "TYPO3",
    "cli",
    "console",
    "command",
    "commandline"
  ],
  "homepage": "https://garv.in/",
  "support": {
    "issues": "https://github.com/garvinhicking/clinspector_gadget",
    "source": "https://github.com/garvinhicking/clinspector_gadget",
    "docs": "https://github.com/garvinhicking/clinspector_gadget"
  },
  "authors": [
    {
      "name": "Garvin Hicking",
      "email": "mail@garv.in",
      "role": "Developer",
      "homepage": "https://garv.in"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1",
    "typo3/cms-backend": "^11.5.26 || ^12.4 || dev-main",
    "typo3/cms-composer-installers": "^4.0@rc || >=5.0",
    "typo3/cms-core": "^11.5.26 || ^12.4 || dev-main",
    "symfony/console": "^5.4 || ^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Garvinhicking\\ClinspectorGadget\\": [
        "Classes/Command/"
      ]
    }
  },
  "replace": {
    "typo3-ter/clinspector-gadget": "self.version"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "clinspector_gadget"
    }
  }
}
